{
  "name": "www",
  "description": "My personal site, rebuilt using Astro",
  "author": "Xander Low <xander@xandydandy.com> (https://www.xandydandy.com)",
  "type": "module",
  "module": "commonjs",
  "version": "2.1.0",
  "scripts": {
    "prepare": "panda codegen",
    "dev": "rm -rf .astro/ && astro dev --port 3000",
    "dev:studio": "panda studio --preview",
    "start": "astro dev",
    "build": "astro build",
    "build:studio": "panda studio --build",
    "preview": "astro preview",
    "astro": "astro",
    "schema": "pnpx tsx ./schema.ts",
    "format": "prettier --write . --plugin prettier-plugin-astro",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx,astro}\"",
    "lint:fix": "eslint \"**/*.{js,jsx,ts,tsx,astro}\" --fix",
    "sketch:square": "hygen square new --name",
    "sketch:a0": "hygen a0 new --name",
    "sketch:a1": "hygen a1 new --name",
    "sketch:a2": "hygen a2 new --name",
    "sketch:a3": "hygen a3 new --name",
    "sketch:a4": "hygen a4 new --name",
    "sketch:iphone-15": "hygen iphone-15 new --name",
    "sketch:iphone-pro-max": "hygen iphone-pro-max new --name",
    "sketch:legal": "hygen legal new --name",
    "sketch:letter": "hygen letter new --name",
    "sketch:shader": "hygen shader new --name"
  },
  "dependencies": {
    "@astrojs/mdx": "1.1.3",
    "@astrojs/solid-js": "3.0.2",
    "@astrojs/vercel": "5.1.0",
    "@auth/core": "0.18.0",
    "@fontsource/dm-mono": "5.0.17",
    "@fontsource/dm-serif-text": "5.0.17",
    "@jlengstorf/get-share-image": "0.8.0",
    "@nanostores/persistent": "^0.9.1",
    "@nanostores/solid": "0.4.2",
    "@vercel/blob": "0.14.1",
    "astro": "3.4.3",
    "auth-astro": "3.0.1",
    "change-case": "5.1.2",
    "fetch-base64": "2.1.2",
    "fuse.js": "7.0.0",
    "jspdf": "2.5.1",
    "nanostores": "0.9.4",
    "p5": "1.6.0",
    "p5.js-svg": "1.5.1",
    "sharp": "0.32.6",
    "solid-js": "1.8.5",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@pandacss/astro": "0.18.0",
    "@pandacss/dev": "0.18.0",
    "@pandacss/types": "0.18.0",
    "@types/geojson": "^7946.0.12",
    "@types/p5": "1.6.0",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "buffer": "6.0.3",
    "eslint": "8.53.0",
    "eslint-plugin-astro": "0.29.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "3.1.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "hygen": "6.2.11",
    "prettier": "3.0.3",
    "prettier-plugin-astro": "0.12.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vite": "4.5.0",
    "zod-to-json-schema": "3.21.4"
  },
  "trustedDependencies": [
    "sharp"
  ]
}
