{
  "name": "www",
  "description": "My personal site, rebuilt using Astro",
  "author": "Xander Low <xander@xandydandy.com> (https://www.xandydandy.com)",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "prepare": "panda codegen",
    "dev": "astro dev --port 3000",
    "start": "astro dev",
    "build": "rm -rf styled-system && pnpm prepare && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx,astro}\"",
    "lint:fix": "eslint \"**/*.{js,jsx,ts,tsx,astro}\" --fix",
    "sketch:square": "npx hygen square new --name",
    "sketch:a0": "npx hygen a0 new --name",
    "sketch:a1": "npx hygen a1 new --name",
    "sketch:a2": "npx hygen a2 new --name",
    "sketch:a3": "npx hygen a3 new --name",
    "sketch:a4": "npx hygen a4 new --name"
  },
  "dependencies": {
    "@astrojs/mdx": "1.1.0",
    "@astrojs/solid-js": "3.0.1",
    "@astrojs/vercel": "5.0.1",
    "@fontsource/dm-mono": "5.0.12",
    "@fontsource/dm-serif-text": "5.0.12",
    "@jlengstorf/get-share-image": "0.8.0",
    "@nanostores/solid": "0.4.2",
    "astro": "3.1.4",
    "change-case": "4.1.2",
    "fuse.js": "6.6.2",
    "nanostores": "0.9.3",
    "p5": "1.6.0",
    "p5.js-svg": "1.5.1",
    "pdfkit": "0.13.0",
    "solid-js": "1.7.12",
    "svg-to-pdfkit": "0.1.8"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "1.1.3",
    "@pandacss/astro": "0.15.2",
    "@pandacss/dev": "0.15.2",
    "@types/p5": "1.6.0",
    "@types/pdfkit": "0.12.12",
    "@types/svg-to-pdfkit": "0.1.1",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "eslint": "8.50.0",
    "eslint-plugin-astro": "0.29.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "prettier": "3.0.3",
    "prettier-plugin-astro": "0.12.0",
    "typescript": "5.2.2"
  }
}
