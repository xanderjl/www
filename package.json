{
  "name": "www",
  "description": "My personal site, rebuilt using Astro",
  "author": "Xander Low <xander@xandydandy.com> (https://www.xandydandy.com)",
  "type": "module",
  "module": "commonjs",
  "version": "2.2.0",
  "scripts": {
    "prepare": "panda codegen",
    "dev": "rm -rf .astro/ && astro dev --port 3000",
    "dev:studio": "panda studio --preview",
    "start": "astro dev",
    "build": "astro build",
    "build:studio": "panda studio --build",
    "preview": "astro preview",
    "astro": "astro",
    "schema": "pnpm dlx tsx ./schema.ts",
    "format": "prettier --write . --plugin prettier-plugin-astro",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx,astro}\"",
    "lint:fix": "eslint \"**/*.{js,jsx,ts,tsx,astro}\" --fix",
    "sketch:square": "hygen square new --name",
    "sketch:a0": "hygen a0 new --name",
    "sketch:a1": "hygen a1 new --name",
    "sketch:a2": "hygen a2 new --name",
    "sketch:a3": "hygen a3 new --name",
    "sketch:a4": "hygen a4 new --name",
    "sketch:iphone-15": "hygen iphone-15 new --name",
    "sketch:iphone-pro-max": "hygen iphone-pro-max new --name",
    "sketch:legal": "hygen legal new --name",
    "sketch:letter": "hygen letter new --name",
    "sketch:shader": "hygen shader new --name"
  },
  "dependencies": {
    "@astrojs/mdx": "2.1.1",
    "@astrojs/solid-js": "4.0.1",
    "@astrojs/vercel": "7.3.4",
    "@auth/core": "0.28.0",
    "@fontsource/dm-mono": "5.0.18",
    "@fontsource/dm-serif-text": "5.0.18",
    "@jlengstorf/get-share-image": "1.0.2",
    "@nanostores/persistent": "0.10.1",
    "@nanostores/solid": "0.4.2",
    "@vercel/blob": "0.22.1",
    "astro": "4.4.9",
    "auth-astro": "4.1.1",
    "change-case": "5.4.3",
    "dat.gui": "^0.7.9",
    "fetch-base64": "2.1.2",
    "fuse.js": "7.0.0",
    "jspdf": "2.5.1",
    "mixbox": "2.0.0",
    "motion": "^10.17.0",
    "nanostores": "0.10.0",
    "nice-color-palettes": "^3.0.0",
    "p5": "1.6.0",
    "p5.js-svg": "1.5.1",
    "sharp": "0.33.2",
    "solid-js": "1.8.15",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@pandacss/astro": "0.25.0",
    "@pandacss/dev": "0.33.0",
    "@pandacss/types": "0.33.0",
    "@types/dat.gui": "^0.7.12",
    "@types/geojson": "7946.0.14",
    "@types/p5": "1.6.0",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "buffer": "6.0.3",
    "eslint": "8.57.0",
    "eslint-plugin-astro": "0.31.4",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "3.2.0",
    "eslint-plugin-unused-imports": "3.1.0",
    "hygen": "6.2.11",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "vite": "5.1.4",
    "zod-to-json-schema": "3.22.4"
  },
  "trustedDependencies": [
    "sharp"
  ]
}
