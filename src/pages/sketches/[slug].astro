---
import Layout from '@/layouts/Layout.astro';
import type { GetStaticPaths } from 'astro';

export const prerender = true;

export const getStaticPaths: GetStaticPaths = async () => {
  return [
    {
      params: {
        slug: 'hello-world',
      },
      props: {
        title: 'Hello World',
      },
    },
  ];
};

interface Props {
  title?: string;
}

const { title } = Astro.props;
---

<Layout title={title} />
