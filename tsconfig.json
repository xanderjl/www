{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "plugins": [
      {
        "name": "@astrojs/ts-plugin",
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "styled-system/*": [
        "styled-system/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}